<fx:root type="javafx.scene.layout.AnchorPane" fx:id="apCustomerListBasePane" focusTraversable="false" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="at.ac.tuwien.inso.tl.client.gui.controller.CustomerListBaseController">
  <TitledPane fx:id="tpTitlePane" animated="true" collapsible="false" focusTraversable="false" text="%customerpage.search_results" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
    <content>
      <AnchorPane>
        <children>
          <TableView fx:id="tvCustomersFoundView" disable="false" focusTraversable="true" prefHeight="200.0" prefWidth="-1.0" tableMenuButtonVisible="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
